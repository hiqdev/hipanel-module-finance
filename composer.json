    "require": {
        "yiisoft/yii2": "^2.0",
        "hiqdev/yii2-cart": "<2.0 || dev-master",
        "hiqdev/yii2-merchant": "<2.0 || dev-master",
        "hiqdev/hipanel-core": "<2.0 || dev-master",
        "hiqdev/omnipay-ecoin": "*@dev",
        "hiqdev/omnipay-epayments": "*@dev",
        "hiqdev/omnipay-epayservice": "*@dev",
        "hiqdev/omnipay-interkassa": "*@dev",
        "hiqdev/omnipay-freekassa": "*@dev",
        "hiqdev/omnipay-robokassa": "*@dev",
        "hiqdev/omnipay-okpay": "*@dev",
        "hiqdev/omnipay-qiwi": "*@dev",
        "hiqdev/omnipay-paxum": "*@dev",
        "hiqdev/omnipay-paypal": "*@dev",
        "hiqdev/omnipay-yandexmoney": "*@dev",
        "dercoder/omnipay-webmoney": "*@dev",
        "omnipay/bitpay": "*@dev",
        "hiqdev/hipanel-module-client": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hipanel\\modules\\finance\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "web": "src/config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
